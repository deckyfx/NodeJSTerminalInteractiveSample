{
  "name": "suitetreat",
  "version": "1.0.0",
  "description": "Suitetreat Resource Fetcher",
  "main": "build/index.js",
  "bin": {
    "suitetreat": "./bin/suitetreat"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "echo \"[]\" > caches/hotel_search.json",
    "build-sqlite": "npm install sqlite3 --build-from-source=sqlite3 --save",
    "compile": "./node_modules/.bin/tsc -p tsconfig.json && npm run copy-public",
    "copy-public": "./bin/copy-public.sh",
    "compile-win": "node_modules\\.bin\\tsc.cmd -p tsconfig.json && npm run copy-public-win",
    "copy-public-win-ps": "./bin/copy-public-win-ps.bat",
    "copy-public-win": "./bin/copy-public-win.bat",
    "cleanbuild": "rm -r ./build/*",
    "movebuild": "mv ./build/src/* ./build/",
    "start": "node ./build/index.js"
  },
